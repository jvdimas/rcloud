## The function is currently defined as
function (seed) 
{
    set.seed(seed)
    numDraws <- 1e+06
    r <- 0.5
    x <- runif(numDraws, min = -r, max = r)
    y <- runif(numDraws, min = -r, max = r)
    inCircle <- ifelse((x^2 + y^2)^0.5 < r, 1, 0)
    return(sum(inCircle)/length(inCircle) * 4)
  }
}
